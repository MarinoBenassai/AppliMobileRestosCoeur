{
  "expo": {
    "name": "Bénévole aux Restos du Cœur - AD 31",
    "slug": "AwesomeProject",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "fr.restosducoeur31-data.mobileapp",
	  "infoPlist": {
  		"LSApplicationQueriesSchemes": ["safari", "chrome"]
	  }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "fr.restosducoeur31_data.mobileapp",
      "versionCode": 5,
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "ACCESS_NOTIFICATION_POLICY"
      ]
	  ,"jsEngine": "hermes"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "owner": "stajcatra",
  }
}
